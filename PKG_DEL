dirlist        = {"examples", "initialize", "hierarchical_classes", ...
          "adaptivity_iga", "adaptivity_iga/refine", "adaptivity_iga/coarsen", ...
          "adaptivity_iga/refine/admissible", "adaptivity_iga/general", ...
	  "adaptivity_iga/solve", "adaptivity_iga/solve/cahn_hilliard"};

dir = fileparts (mfilename ("fullpath"));

for my_ppp=1:length(dirlist)
  rmpath ( [ canonicalize_file_name([dir]) "/" dirlist{my_ppp}])
endfor

clear dirlist dir my_ppp
